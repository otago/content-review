{
    "name": "otago/content-review",
    "description": "Content Review for SS CMS",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "Content Review",
        "CMS"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "silverstripe/framework": "^4.0",
        "silverstripe/admin": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "OP\\": "src/",
            "OP\\Tests\\": "tests/"
        }
    },
    "extra": {
        "expose": [
            "client/dist",
            "css"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
